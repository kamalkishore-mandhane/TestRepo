    <Style x:Uid="Style_21" x:Key="ComboBoxFlatStyle"  TargetType="{x:Type ComboBox}">
        <Setter x:Uid="Setter_60" Property="UIElement.SnapsToDevicePixels" Value="True"/>
        <Setter x:Uid="Setter_61" Property="FrameworkElement.OverridesDefaultStyle" Value="True"/>
        <Setter x:Uid="Setter_62" Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
        <Setter x:Uid="Setter_63" Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter x:Uid="Setter_64" Property="ScrollViewer.CanContentScroll" Value="True"/>
        <Setter x:Uid="Setter_65" Property="TextElement.Foreground" Value="{DynamicResource Brush.Text.Foreground}"/>
        <Setter x:Uid="Setter_66" Property="FrameworkElement.FocusVisualStyle" Value="{DynamicResource FocusVisual}"/>
        <Setter x:Uid="Setter_67" Property="Control.Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_12" TargetType="ComboBox">
                    <Grid x:Uid="Grid_5" >
                        <ToggleButton x:Uid="ToggleButton" Name="ToggleButton" Grid.Column="2"
                                      IsChecked="{Binding Path=IsDropDownOpen, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                      ClickMode="Press" Focusable="False" Template="{StaticResource ComboBoxToggleButtonTemplate}"/>
                        <ContentPresenter x:Uid="ContentSite" Name="ContentSite" Margin="5,3,23,3"
                                          HorizontalAlignment="Left" VerticalAlignment="Center" IsHitTestVisible="False"
                                          Content="{TemplateBinding ComboBox.SelectionBoxItem}"
                                          ContentTemplate="{TemplateBinding ComboBox.SelectionBoxItemTemplate}"
                                          ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}"/>
                        <TextBox x:Uid="PART_EditableTextBox" Name="PART_EditableTextBox" Margin="3, 3, 23, 3" Focusable="True"
                                IsReadOnly="{TemplateBinding IsReadOnly}" Visibility="Hidden" Background="Transparent" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBox.Template>
                                <ControlTemplate x:Uid="ControlTemplate_13" TargetType="TextBox" >
                                    <Border x:Uid="PART_ContentHost" Name="PART_ContentHost" Focusable="False"/>
                                </ControlTemplate>
                            </TextBox.Template>
                        </TextBox>
                        <!-- Popup showing items -->
                        <Popup x:Uid="Popup" Name="Popup" Placement="Bottom"
                            Focusable="False" AllowsTransparency="True" IsOpen="{TemplateBinding ComboBox.IsDropDownOpen}" PopupAnimation="None">
                            <Grid x:Uid="DropDown" Name="DropDown" SnapsToDevicePixels="True"
                                  MinWidth="{TemplateBinding FrameworkElement.ActualWidth}"
                                  MaxHeight="{TemplateBinding ComboBox.MaxDropDownHeight}">
                                <Border x:Uid="DropDownBorder" Name="DropDownBorder" Background="{DynamicResource Brush.Background.White}" Margin="0,1,0,0"
                                        CornerRadius="0" BorderThickness="1,1,1,1" BorderBrush="{DynamicResource Brush.ComboBox.Border.Normal}"/>
                                <ScrollViewer x:Uid="ScrollViewer_1" Margin="4" SnapsToDevicePixels="True">
                                    <ItemsPresenter x:Uid="ItemsPresenter_1" KeyboardNavigation.DirectionalNavigation="Contained"/>
                                </ScrollViewer>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_6" Property="ItemsControl.HasItems" Value="False">
                            <Setter x:Uid="Setter_68" Property="FrameworkElement.MinHeight" TargetName="DropDownBorder" Value="95"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_7" Property="UIElement.IsEnabled" Value="False">
                            <Setter x:Uid="Setter_69" Property="TextElement.Foreground" Value="{DynamicResource Brush.ComboBox.Foreground.Disabled}"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_8" Property="ItemsControl.IsGrouping" Value="True">
                            <Setter x:Uid="Setter_70" Property="ScrollViewer.CanContentScroll" Value="False"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_9" Property="ComboBox.IsEditable" Value="True">
                            <Setter x:Uid="Setter_71" Property="KeyboardNavigation.IsTabStop" Value="False"/>
                            <Setter x:Uid="Setter_72" Property="UIElement.Visibility" TargetName="PART_EditableTextBox" Value="Visible"/>
                            <Setter x:Uid="Setter_73" Property="UIElement.Visibility" TargetName="ContentSite" Value="Hidden"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!-- End of Flat ComboBox -->

