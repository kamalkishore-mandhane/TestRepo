    <!-- Flat ComboBox -->
    <ControlTemplate x:Uid="ControlTemplate_11" TargetType="ToggleButton" x:Key="ComboBoxToggleButtonTemplate">
        <Grid x:Uid="Grid_4" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition x:Uid="ColumnDefinition_1" Width="*"/>
                <ColumnDefinition x:Uid="ColumnDefinition_2" Width="20" />
            </Grid.ColumnDefinitions>
            <Border x:Uid="Border_6" Grid.ColumnSpan="2" Name="Border"
                BorderBrush="{DynamicResource Brush.ComboBox.Border.Normal}" CornerRadius="0" BorderThickness="1"
                Background="{DynamicResource Brush.ComboBox.Background.Normal}" />
            <Border x:Uid="Border_7" Grid.Column="1" Margin="1,1,1,1" BorderBrush="{DynamicResource Brush.ComboBox.Border.Light}" Name="ButtonBorder"
                CornerRadius="0" BorderThickness="0" Background="{DynamicResource Brush.ComboBox.Background.Normal}" />
            <Path x:Uid="Arrow" Name="Arrow" Grid.Column="1" Data="M0,0 L0,2 L4,6 L8,2 L8,0 L4,4 z"
                HorizontalAlignment="Center" Fill="{DynamicResource Brush.Icon.BasicColor}" VerticalAlignment="Center" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger x:Uid="Trigger_4" Property="ToggleButton.IsChecked" Value="True">
                <Setter x:Uid="Setter_53" Property="Panel.Background" TargetName="ButtonBorder" Value="{DynamicResource Brush.Background.White}"/>
                <Setter x:Uid="Setter_54" Property="Shape.Fill" TargetName="Arrow" Value="{DynamicResource Brush.ComboBox.Arrow.Checked}"/>
            </Trigger>
            <Trigger x:Uid="Trigger_5" Property="UIElement.IsEnabled" Value="False">
                <Setter x:Uid="Setter_55" Property="Panel.Background" TargetName="Border" Value="{DynamicResource Brush.ComboBox.Background.Disabled}"/>
                <Setter x:Uid="Setter_56" Property="Panel.Background" TargetName="ButtonBorder" Value="{DynamicResource Brush.ComboBox.Background.Disabled}"/>
                <Setter x:Uid="Setter_57" Property="Border.BorderBrush" TargetName="ButtonBorder" Value="{DynamicResource Brush.ComboBox.Border.Disabled}"/>
                <Setter x:Uid="Setter_58" Property="TextElement.Foreground" Value="{DynamicResource Brush.ComboBox.Foreground.Disabled}"/>
                <Setter x:Uid="Setter_59" Property="Shape.Fill" TargetName="Arrow" Value="{DynamicResource Brush.ComboBox.Arrow.IsEnabled}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Uid="Style_8" x:Key="FocusVisual">
        <Setter x:Uid="Setter_24" Property="Control.Template">
