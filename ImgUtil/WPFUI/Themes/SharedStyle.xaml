                    <Grid x:Uid="Grid_2" >
                        <Border x:Uid="FrameRect" Name="FrameRect" CornerRadius="0" BorderThickness="1" SnapsToDevicePixels="True" BorderBrush="{DynamicResource Brush.Title}" Visibility="Hidden"/>
                        <Border x:Uid="ButtonBorder" Name="ButtonBorder" CornerRadius="0" BorderThickness="1" SnapsToDevicePixels="True" BorderBrush="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}" Margin="2">
                            <Rectangle x:Uid="Rectangle_2" Fill="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"/>
