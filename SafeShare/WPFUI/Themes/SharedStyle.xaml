    <Style x:Uid="Style_1" x:Key="TextToolTip" TargetType="{x:Type ToolTip}">
        <Setter x:Uid="Setter_1" Property="OverridesDefaultStyle" Value="true" />
        <Setter x:Uid="Setter_2" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_3" Property="HasDropShadow" Value="True" />
        <Setter x:Uid="Setter_4" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_1" TargetType="{x:Type ToolTip}">
                    <StackPanel x:Uid="StackPanel_1">
                        <Border x:Uid="Border_1" CornerRadius="0"
                            SnapsToDevicePixels="True"
                            BorderThickness="1"
                            Margin="2"
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            BorderBrush="{DynamicResource Brush.Tooltip.Border}"
                            Background="{DynamicResource Brush.Tooltip.Background}">
                            <ContentPresenter x:Uid="ContentPresenter_1" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Top" />
                        </Border>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_2" x:Key="ToolTipTextBlock" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_6" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_7" Property="Foreground" Value="{DynamicResource Brush.Tooltip.Text}" />
        <Setter x:Uid="Setter_8" Property="FontFamily" Value="{StaticResource Shared.FontFamily.SegoeUI}" />
        <Setter x:Uid="Setter_9" Property="FontSize" Value="{StaticResource Shared.FontSize.12px}" />
    </Style>

    <Style x:Uid="Style_42" x:Key="MainTitleTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_203" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_204" Property="FontSize" Value="{StaticResource Shared.FontSize.15px}" />
        <Setter x:Uid="Setter_205" Property="FontWeight" Value="{StaticResource Shared.FontWeight.SemiBold}" />
        <Setter x:Uid="Setter_206" Property="Foreground" Value="{DynamicResource Brush.Text.Black.Foreground}" />
