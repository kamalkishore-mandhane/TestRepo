        <Setter x:Uid="Setter_12" Property="FontWeight" Value="{StaticResource Shared.FontWeight.SemiBold}" />
        <Setter x:Uid="Setter_13" Property="Foreground" Value="{DynamicResource Brush.Text.Black.Foreground}" />
    </Style>

    <Style x:Uid="Style_4" x:Key="SubTitleTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_14" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_15" Property="FontSize" Value="{StaticResource Shared.FontSize.11px}" />
        <Setter x:Uid="Setter_16" Property="FontWeight" Value="{StaticResource Shared.FontWeight.SemiBold}" />
        <Setter x:Uid="Setter_17" Property="Foreground" Value="{DynamicResource Brush.Text.Black.Foreground}" />
    </Style>

    <Style x:Uid="Style_5" x:Key="BlackContentTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_18" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_19" Property="FontSize" Value="{StaticResource Shared.FontSize.13px}" />
        <Setter x:Uid="Setter_20" Property="FontWeight" Value="{StaticResource Shared.FontWeight.SemiBold}" />
        <Setter x:Uid="Setter_21" Property="Foreground" Value="{DynamicResource Brush.Text.Black.Foreground}" />
    </Style>

    <Style x:Uid="Style_6" x:Key="GrayContentTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_22" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_23" Property="FontSize" Value="{StaticResource Shared.FontSize.11px}" />
        <Setter x:Uid="Setter_24" Property="FontWeight" Value="{StaticResource Shared.FontWeight.Normal}" />
        <Setter x:Uid="Setter_25" Property="Foreground" Value="{DynamicResource Brush.Text.Gray.Foreground}" />
    </Style>

    <Style x:Uid="Style_40" x:Key="SeeAllTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_191" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_192" Property="FontSize" Value="{StaticResource Shared.FontSize.12px}" />
        <Setter x:Uid="Setter_193" Property="FontWeight" Value="{StaticResource Shared.FontWeight.Normal}" />
        <Setter x:Uid="Setter_194" Property="Foreground" Value="{DynamicResource Brush.Text.Gray.Foreground}" />
    </Style>

    <Style x:Uid="Style_32" x:Key="RedContentTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_152" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_153" Property="FontSize" Value="{StaticResource Shared.FontSize.11px}" />
        <Setter x:Uid="Setter_154" Property="FontWeight" Value="{StaticResource Shared.FontWeight.Normal}" />
        <Setter x:Uid="Setter_155" Property="Foreground" Value="{DynamicResource Brush.Text.Red.Foreground}" />
    </Style>

    <Style x:Uid="Style_31" x:Key="NormalBlackContentTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_147" Property="FontSize" Value="{StaticResource Shared.FontSize.13px}" />
        <Setter x:Uid="Setter_148" Property="FontWeight" Value="{StaticResource Shared.FontWeight.Normal}" />
        <Setter x:Uid="Setter_149" Property="Foreground" Value="{DynamicResource Brush.Text.Black.Foreground}" />
        <Setter x:Uid="Setter_150" Property="SnapsToDevicePixels" Value="True" />
        <Style.Triggers>
            <Trigger x:Uid="Trigger_24" Property="IsEnabled" Value="False">
                <Setter x:Uid="Setter_151" Property="Foreground" Value="{DynamicResource Brush.Text.Gray.Foreground}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Uid="Style_7" x:Key="RoundCornerBlackButtonTextStyle" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_26" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_27" Property="FontSize" Value="{StaticResource Shared.FontSize.13px}" />
        <Setter x:Uid="Setter_28" Property="FontWeight" Value="{StaticResource Shared.FontWeight.Normal}" />
        <Setter x:Uid="Setter_29" Property="Foreground" Value="{DynamicResource Brush.Text.White.Foreground}" />
    </Style>

    <Style x:Uid="Style_8" x:Key="ManageServiceListViewItemContainerStyle" TargetType="{x:Type ListViewItem}">
        <Setter x:Uid="Setter_30" Property="Margin" Value="15,4" />
        <Setter x:Uid="Setter_31" Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter x:Uid="Setter_32" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_2" TargetType="{x:Type ListViewItem}">
                    <Border x:Uid="ItemContainerBorder" x:Name="ItemContainerBorder"
                            MinHeight="45"
                            Padding="10,0"
                            BorderBrush="{DynamicResource  Brush.Window.Gray.Border}"
                            BorderThickness="1"
                            CornerRadius="5"
                            SnapsToDevicePixels="True"
                            Background="{DynamicResource Brush.Window.White.Background}">
                        <ContentPresenter x:Uid="ContentPresenter_2" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_1" Property="IsMouseOver" Value="True">
                            <Setter x:Uid="Setter_33" Property="Background" Value="{DynamicResource Brush.ListView.Item.Hover}" TargetName="ItemContainerBorder" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_2" Property="IsSelected" Value="True">
                            <Setter x:Uid="Setter_34" Property="BorderBrush" Value="{DynamicResource Brush.ListView.Item.Selected.Blue}" TargetName="ItemContainerBorder" />
                        </Trigger>

                        <EventTrigger x:Uid="EventTrigger_1" RoutedEvent="Loaded">
                            <BeginStoryboard x:Uid="BeginStoryboard_1">
                                <Storyboard x:Uid="Storyboard_1">
                                    <DoubleAnimation x:Uid="DoubleAnimation_1" Duration="0:0:0.2" Storyboard.TargetProperty="Opacity" From="0.3" To="1" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_9" x:Key="PageWindowBorder" TargetType="{x:Type Border}">
        <Setter x:Uid="Setter_202" Property="Opacity" Value="0.9" />
        <Setter x:Uid="Setter_35" Property="CornerRadius" Value="9" />
        <Setter x:Uid="Setter_36" Property="BorderThickness" Value="1" />
        <Setter x:Uid="Setter_37" Property="BorderBrush" Value="{DynamicResource Brush.Window.Gray.Border}" />
        <Setter x:Uid="Setter_38" Property="Background" Value="{DynamicResource Brush.Window.White.Background}" />
        <Setter x:Uid="Setter_39" Property="SnapsToDevicePixels" Value="True" />
    </Style>

    <ControlTemplate x:Uid="ControlTemplate_28" x:Key="BlackRadioButtonControlTemplate" TargetType="{x:Type RadioButton}">
        <Border x:Uid="Border_15" SnapsToDevicePixels="True">
            <Grid x:Uid="Grid_23">
                <DockPanel x:Uid="DockPanel_1">
                    <Rectangle x:Uid="R_Ico" x:Name="R_Ico" Margin="3,0,0,0"
                               Width="20" Height="20"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center" Fill="{StaticResource RadioButtonInaDrawBrush}" />
                    <Border x:Uid="Border" x:Name="Border" SnapsToDevicePixels="True"
                            Margin="0,0,5,0"
                            Background="Transparent" BorderBrush="Transparent">
                        <ContentPresenter x:Uid="ContentPresenter_7" Margin="0,0,0,0" />
                    </Border>
                </DockPanel>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger x:Uid="Trigger_32" Property="IsChecked" Value="True">
                <Setter x:Uid="Setter_184" Property="Fill" Value="{StaticResource RadioButtonDrawBrush}" TargetName="R_Ico" />
            </Trigger>
            <Trigger x:Uid="Trigger_33" Property="IsEnabled" Value="False">
                <Setter x:Uid="Setter_185" Property="Fill" Value="{StaticResource RadioButtonDisableDrawBrush}" TargetName="R_Ico" />
            </Trigger>
            <MultiTrigger x:Uid="MultiTrigger_3">
                <MultiTrigger.Conditions>
                    <Condition x:Uid="Condition_5" Property="IsMouseOver" Value="True" />
                    <Condition x:Uid="Condition_6" Property="IsChecked" Value="True" />
                </MultiTrigger.Conditions>
                <Setter x:Uid="Setter_207" Property="Fill" Value="{StaticResource RadioButtonHoverDrawBrush}" TargetName="R_Ico" />
            </MultiTrigger>
            <MultiTrigger x:Uid="MultiTrigger_4">
                <MultiTrigger.Conditions>
                    <Condition x:Uid="Condition_7" Property="IsMouseOver" Value="True" />
                    <Condition x:Uid="Condition_8" Property="IsChecked" Value="False" />
                </MultiTrigger.Conditions>
                <Setter x:Uid="Setter_208" Property="Fill" Value="{StaticResource RadioButtonInaHoverDrawBrush}" TargetName="R_Ico" />
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Uid="Style_39" x:Key="BlackRadioButtonStyle" TargetType="{x:Type RadioButton}">
        <Setter x:Uid="Setter_187" Property="HorizontalAlignment" Value="Left" />
        <Setter x:Uid="Setter_188" Property="Template" Value="{StaticResource BlackRadioButtonControlTemplate}" />
    </Style>

    <ControlTemplate x:Uid="ControlTemplate_4" x:Key="ManageServiceListViewButtonTemplate" TargetType="{x:Type Button}">
        <Border x:Uid="RoundedBorder" x:Name="RoundedBorder"
                Background="Transparent"
                BorderBrush="{DynamicResource Brush.ListView.Item.Button.Hover}"
                BorderThickness="0"
                CornerRadius="5">
            <StackPanel x:Uid="StackPanel_2" Orientation="Horizontal" Margin="2,5">
                <TextBlock x:Uid="TextBlock_1" Grid.Column="3"
                           Margin="5,0"
                           MaxWidth="145"
                           TextWrapping="Wrap"
                           TextAlignment="Right"
                           TextTrimming="CharacterEllipsis"
                           ToolTip="{Binding SelectedAccount.DisplayName}"
                           Text="{Binding SelectedAccount.DisplayName}"
                           VerticalAlignment="Center" HorizontalAlignment="Right" />

                <Path x:Uid="ExpandededArrow" x:Name="ExpandededArrow"
                      Width="8"
                      Height="8"
                      Stretch="Uniform"
                      SnapsToDevicePixels="True"
                      StrokeThickness="1"
                      Stroke="{DynamicResource Brush.Text.Gray.Foreground}"
                      HorizontalAlignment="Center"
                      VerticalAlignment="Center"
                      Data="M 0 4 L 4 0 L 0 -4" />
            </StackPanel>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger x:Uid="Trigger_5" Property="IsMouseOver" Value="True">
                <Setter x:Uid="Setter_43" TargetName="RoundedBorder" Property="BorderThickness" Value="1" />
            </Trigger>
            <Trigger x:Uid="Trigger_37" Property="IsPressed" Value="True">
                <Setter x:Uid="Setter_195" TargetName="RoundedBorder" Property="Background" Value="{DynamicResource Brush.White.Button.Backgroung}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Uid="DataTemplate_1" x:Key="ManageServiceListViewItemTemplate">
        <Grid x:Uid="Grid_1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition x:Uid="ColumnDefinition_3" Width="*" />
                <ColumnDefinition x:Uid="ColumnDefinition_4" Width="Auto" />
            </Grid.ColumnDefinitions>

            <RadioButton x:Uid="RadioButton_1" Grid.Column="0"
                         AutomationProperties.Name="{Binding DisplayName}"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Left"
                         Margin="5,0,0,0"
                         GroupName="ServiceRadioButton"
                         FocusVisualStyle="{x:Null}"
                         IsChecked="{Binding Path=IsSelected, RelativeSource={RelativeSource AncestorType={x:Type ListViewItem}}}"
                         Template="{StaticResource BlackRadioButtonControlTemplate}">
                <StackPanel x:Uid="StackPanel_6" Orientation="Horizontal">
                    <Image x:Uid="Image_1"
                           Source="{Binding Icon}"
                           Height="22" Width="22"
                           Margin="14,0,6,0"
                           VerticalAlignment="Center" />
                    <TextBlock x:Uid="TextBlock_2"
                               MaxWidth="122"
                               VerticalAlignment="Center"
                               Text="{Binding DisplayName}"
                               TextWrapping="Wrap"
                               Style="{StaticResource BlackContentTextBlockStyle}" />
                </StackPanel>
            </RadioButton>

            <Button x:Uid="Button_1" Grid.Column="1"
                    AutomationProperties.Name="{Binding SelectedAccount.DisplayName}"
                    VerticalAlignment="Center" HorizontalContentAlignment="Center"
                    Template="{StaticResource ManageServiceListViewButtonTemplate}"
                    Command="{Binding DataContext.ManageAccounts, RelativeSource={RelativeSource AncestorType=ListView}}"
                    CommandParameter="{Binding}" />
        </Grid>
    </DataTemplate>

    <ControlTemplate x:Uid="VerticalScrollBarTemplate" x:Key="VerticalScrollBarTemplate"
                             TargetType="{x:Type ScrollBar}"
                             x:Name="VerticalScrollBarTemplate">
        <Grid x:Uid="CustomScrollBarGrid" x:Name="CustomScrollBarGrid" Width="8" Focusable="False">
            <Grid.RowDefinitions>
                <RowDefinition x:Uid="RowDefinition_1" Height="Auto" />
                <RowDefinition x:Uid="RowDefinition_2" Height="*" />
                <RowDefinition x:Uid="RowDefinition_3" Height="Auto" />
            </Grid.RowDefinitions>

            <Border x:Uid="TrackBorder" x:Name="TrackBorder"
                            BorderThickness="0"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Grid.Row="1">
                <Track x:Uid="PART_Track" x:Name="PART_Track" Grid.Row="1"
                           Width="6"
                           IsDirectionReversed="True">
                    <Track.DecreaseRepeatButton>
                        <RepeatButton x:Uid="RepeatButton_1" BorderThickness="0"
                                      Background="Transparent"
                                      Command="ScrollBar.PageUpCommand" />
                    </Track.DecreaseRepeatButton>

                    <Track.Thumb>
                        <Thumb x:Uid="TrackThumb" x:Name="TrackThumb"
                               HorizontalAlignment="Right"
                               Width="2">
                            <Thumb.Template>
                                <ControlTemplate x:Uid="ControlTemplate_5" TargetType="{x:Type Thumb}">
                                    <Border x:Uid="Border_2" CornerRadius="4" Background="{DynamicResource Brush.ScrollBar.Background}" />
                                </ControlTemplate>
                            </Thumb.Template>
                        </Thumb>
                    </Track.Thumb>

                    <Track.IncreaseRepeatButton>
                        <RepeatButton x:Uid="RepeatButton_2" BorderThickness="0"
                                      Background="Transparent"
                                      Command="ScrollBar.PageDownCommand" />
                    </Track.IncreaseRepeatButton>
                </Track>
            </Border>
        </Grid>

        <ControlTemplate.Triggers>
            <EventTrigger x:Uid="EventTrigger_2" RoutedEvent="MouseEnter">
                <BeginStoryboard x:Uid="BeginStoryboard_2">
                    <Storyboard x:Uid="Storyboard_2">
                        <DoubleAnimation x:Uid="DoubleAnimation_2" Duration="0:0:0.1" Storyboard.TargetName="TrackThumb" Storyboard.TargetProperty="Width" To="6" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
            <EventTrigger x:Uid="EventTrigger_3" RoutedEvent="MouseLeave">
                <BeginStoryboard x:Uid="BeginStoryboard_3">
                    <Storyboard x:Uid="Storyboard_3">
                        <DoubleAnimation x:Uid="DoubleAnimation_3" Duration="0:0:0.1" Storyboard.TargetName="TrackThumb" Storyboard.TargetProperty="Width" To="2" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Uid="ControlTemplate_6" x:Key="ManageServiceListViewTemplate" TargetType="{x:Type ListView}">
        <ScrollViewer x:Uid="ScrollViewer_1" VerticalScrollBarVisibility="Auto"
                      HorizontalScrollBarVisibility="Disabled">
            <ScrollViewer.Template>
                <ControlTemplate x:Uid="ControlTemplate_7" TargetType="{x:Type ScrollViewer}">
                    <Grid x:Uid="Grid_2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Uid="ColumnDefinition_5" Width="*" />
                            <ColumnDefinition x:Uid="ColumnDefinition_6" Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <ScrollContentPresenter x:Uid="ScrollContentPresenter_1" Grid.Column="0" Grid.ColumnSpan="2" />
                        <ScrollBar x:Uid="PART_VerticalScrollBar" Grid.Column="1"
                                   x:Name="PART_VerticalScrollBar"
                                   Margin="0,4"
                                   Value="{TemplateBinding VerticalOffset}"
                                   Maximum="{TemplateBinding ScrollableHeight}"
                                   ViewportSize="{TemplateBinding ViewportHeight}"
                                   Template="{StaticResource VerticalScrollBarTemplate}" />
                    </Grid>
                </ControlTemplate>
            </ScrollViewer.Template>
            <ItemsPresenter x:Uid="ItemsPresenter_1" />
        </ScrollViewer>
    </ControlTemplate>

    <Style x:Uid="Style_14" x:Key="SeeAllButtonStyle" TargetType="{x:Type Button}">
        <Setter x:Uid="Setter_196" Property="AutomationProperties.Name" Value="See all" />
        <Setter x:Uid="Setter_54" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_8" TargetType="{x:Type Button}">
                    <TextBlock x:Uid="TextBlock_3" Text="See All"
                               Margin="3"
                               Style="{StaticResource SeeAllTextBlockStyle}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_43" x:Key="ButtonFocusVisual">
        <Setter x:Uid="Setter_211" Property="Control.Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_30">
                    <Rectangle x:Uid="Rectangle_1" StrokeThickness="1" StrokeDashArray="1.5 1.5" Margin="2" Stroke="{DynamicResource Brush.ButtonFocusVisual.Focus.Border}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_15" x:Key="RoundCornerBlackButtonStyle" TargetType="{x:Type Button}">
        <Setter x:Uid="Setter_212" Property="FocusVisualStyle" Value="{StaticResource ButtonFocusVisual}"/>
        <Setter x:Uid="Setter_55" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_56" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_9" TargetType="{x:Type Button}">
                    <Grid x:Uid="Grid_3">
                        <Border x:Uid="ButtonBorder" Name="ButtonBorder" CornerRadius="4" BorderThickness="1" SnapsToDevicePixels="True"
                                BorderBrush="{DynamicResource Brush.Black.Button.Border}"
                                Background="{DynamicResource Brush.Black.Button.Background}">
                        </Border>
                        <TextBlock x:Uid="TextBlock_4" HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   SnapsToDevicePixels="True"
                                   Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Content}"
                                   Foreground="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Foreground}"
                                   TextAlignment="Center"
                                   Style="{StaticResource RoundCornerBlackButtonTextStyle}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_6" Property="IsMouseOver" Value="True">
                            <Setter x:Uid="Setter_57" Property="Background" TargetName="ButtonBorder" Value="{DynamicResource Brush.Black.Button.Hover.Background}" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_7" Property="IsPressed" Value="True">
                            <Setter x:Uid="Setter_58" Property="Background" TargetName="ButtonBorder" Value="{DynamicResource Brush.Black.Button.Pressed.Background}" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_8" Property="IsEnabled" Value="False">
                            <Setter x:Uid="Setter_59" Property="Background" TargetName="ButtonBorder" Value="{DynamicResource Brush.Black.Button.Disabled.Background}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_36" x:Key="RoundCornerGrayButtonStyle" TargetType="{x:Type Button}">
        <Setter x:Uid="Setter_169" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_170" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_26" TargetType="{x:Type Button}">
                    <Grid x:Uid="Grid_21">
                        <Border x:Uid="Border_13" Name="ButtonBorder" CornerRadius="4" BorderThickness="1" SnapsToDevicePixels="True"
                                BorderBrush="{DynamicResource Brush.Gray.Button.Border}"
                                Background="{DynamicResource Brush.Gray.Button.Background}">
                        </Border>
                        <TextBlock x:Uid="TextBlock_6" HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   SnapsToDevicePixels="True"
                                   Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Content}"
                                   Foreground="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Foreground}"
                                   TextAlignment="Center"
                                   Style="{StaticResource RoundCornerBlackButtonTextStyle}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_27" Property="IsMouseOver" Value="True">
                            <Setter x:Uid="Setter_171" Property="Background" TargetName="ButtonBorder" Value="{DynamicResource Brush.Gray.Button.Hover.Background}" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_28" Property="IsPressed" Value="True">
                            <Setter x:Uid="Setter_172" Property="Background" TargetName="ButtonBorder" Value="{DynamicResource Brush.Gray.Button.Pressed.Background}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
