                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_1" Property="IsHighlighted" Value="True">
                            <Setter x:Uid="Setter_11" TargetName="bd" Property="Background" Value="{DynamicResource Brush.Item.HoverBackground}"/>
                            <Setter x:Uid="Setter_12" TargetName="bd" Property="BorderBrush" Value="{DynamicResource Brush.Item.HoverBorder}"/>
                            <Setter x:Uid="Setter_13" TargetName="tb" Property="Foreground" Value="{DynamicResource Brush.Item.Text.Black.Highlight}"/>
                        </Trigger>
