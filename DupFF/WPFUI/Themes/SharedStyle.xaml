    <Style x:Uid="Style_29" x:Key="StateButtonBaseStyle" TargetType="{x:Type controls:StateButton}">
        <Style.Resources>
            <Style x:Uid="Style_30" x:Key="IconContentGridStyle" TargetType="{x:Type Grid}">
                <Setter x:Uid="Setter_118" Property="Visibility" Value="Visible"/>
                <Setter x:Uid="Setter_119" Property="Margin" Value="0"/>
                <Setter x:Uid="Setter_120" Property="MaxWidth" Value="15"/>
                <Setter x:Uid="Setter_121" Property="MaxHeight" Value="15"/>
            </Style>
            <Style x:Uid="Style_31" x:Key="FocusedBorderStyle" TargetType="{x:Type Border}">
                <Setter x:Uid="Setter_122" Property="Visibility" Value="Collapsed"/>
            </Style>
            <Style x:Uid="Style_32" x:Key="BlinkingIconStyle" TargetType="{x:Type Path}">
                <Setter x:Uid="Setter_123" Property="Visibility" Value="Collapsed"/>
            </Style>
        </Style.Resources>
        <Setter x:Uid="Setter_124" Property="Foreground" Value="{DynamicResource BaseForeground}" />
        <Setter x:Uid="Setter_125" Property="GenericColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_126" Property="MouseOverColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_127" Property="MouseDownColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_128" Property="ChangeColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_129" Property="SelectedHoverColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_130" Property="Background" Value="Transparent" />
        <Setter x:Uid="Setter_131" Property="BorderBrush" Value="Transparent" />
        <Setter x:Uid="Setter_132" Property="BorderThickness" Value="0" />
        <Setter x:Uid="Setter_133" Property="CornerRadius" Value="0"/>
        <Setter x:Uid="Setter_134" Property="Width" Value="15"/>
        <Setter x:Uid="Setter_135" Property="Height" Value="15"/>
        <Setter x:Uid="Setter_136" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_20" TargetType="{x:Type controls:StateButton}">
                    <Grid x:Uid="grid" 
                          SnapsToDevicePixels="True"
                          x:Name="grid">
                        <Border x:Uid="Border_17" Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding CornerRadius}"/>
                        <Grid x:Uid="ContentGrid" x:Name="ContentGrid" 
                              Margin="{TemplateBinding Padding}"
                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition x:Uid="ContentColumn" Width="Auto" x:Name="ContentColumn" />
                                <ColumnDefinition x:Uid="IconColumn" x:Name="IconColumn"/>
                            </Grid.ColumnDefinitions>

                            <ContentPresenter x:Uid="contentPresenter" x:Name="contentPresenter"                                          
                                              HorizontalAlignment="Center"                                          
                                              VerticalAlignment="Center"   
                                              TextBlock.Foreground="{TemplateBinding Foreground}">
                                <ContentPresenter.Resources>
                                    <Style x:Uid="Style_33" TargetType="{x:Type TextBlock}">
                                        <Setter x:Uid="Setter_137" Property="TextTrimming"
                                            Value="CharacterEllipsis" />
                                        <Setter x:Uid="Setter_138" Property="VerticalAlignment"
                                                Value="Center"/>
                                    </Style>
                                </ContentPresenter.Resources>
                            </ContentPresenter>

                            <Grid x:Uid="content" Grid.Column="1"
                                  x:Name="content">
                                <Path x:Uid="DefaultIcon" x:Name="DefaultIcon"
                                      Fill="{TemplateBinding GenericColor}"
                                      Data="{TemplateBinding StateDefaultIcon}"
                                      Stretch="Uniform"
                                      />
                                <Path x:Uid="ChangeIcon" x:Name="ChangeIcon"
                                      Fill="{TemplateBinding ChangeColor}"
                                      Data="{TemplateBinding StateChangeIcon}"
                                      Stretch="Uniform"
                                      Visibility="Collapsed"
                                      />
                                <Path x:Uid="BlinkingIcon" x:Name="BlinkingIcon"
                                      Fill="{TemplateBinding MouseDownColor}"
                                      Data="{TemplateBinding StateDefaultIcon}"
                                      Stretch="Uniform"
                                      Style="{StaticResource BlinkingIconStyle}" 
                                      />
                            </Grid>
                        </Grid>
                    </Grid>

                    <ControlTemplate.Triggers>
                        <MultiTrigger x:Uid="MultiTrigger_1">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_3" Property="IsMouseOver" Value="True" />
                                <Condition x:Uid="Condition_4" Property="State" Value="0" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_139" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseOverColor}"/>
                            <Setter x:Uid="Setter_140" Property="Fill" TargetName="DefaultIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseOverColor}" />
                        </MultiTrigger>

                        <Trigger x:Uid="Trigger_16" Property="IsPressed" Value="true">
                            <Setter x:Uid="Setter_141" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                            <Setter x:Uid="Setter_142" Property="Fill" TargetName="DefaultIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                            <Setter x:Uid="Setter_143" Property="Fill" TargetName="ChangeIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                        </Trigger>

                        <Trigger x:Uid="Trigger_17" Property="State" Value="1">
                            <Setter x:Uid="Setter_144" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                            <Setter x:Uid="Setter_145" Property="Visibility" TargetName="DefaultIcon" Value="Collapsed" />
                            <Setter x:Uid="Setter_146" Property="Visibility" TargetName="ChangeIcon" Value="Visible" />
                        </Trigger>

                        <MultiTrigger x:Uid="MultiTrigger_2">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_5" Property="State" Value="1" />
                                <Condition x:Uid="Condition_6" Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_147" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHoverColor}" />
                            <Setter x:Uid="Setter_148" Property="Fill" TargetName="ChangeIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHoverColor}" />
                        </MultiTrigger>

                        <Trigger x:Uid="Trigger_18" Property="IsEnabled" Value="False">
                            <Setter x:Uid="Setter_149" Property="Opacity" TargetName="ContentGrid" Value="0.3" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

