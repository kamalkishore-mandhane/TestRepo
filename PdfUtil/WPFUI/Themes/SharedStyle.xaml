                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_48" Property="IsChecked" Value="True">
                            <Setter x:Uid="Setter_240" TargetName="cb" Property="Source" Value="{controls:MultiDPIImageSource true}"></Setter>
                            <Setter x:Uid="Setter_241" Property="Background" Value="{DynamicResource Brush.Background.Gray}"/>
                            <Setter x:Uid="Setter_242" Property="BorderBrush" Value="{DynamicResource Brush.Background.Gray}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_47" x:Key="FlatIconCheckBoxStyle" TargetType="{x:Type CheckBox}">
        <Setter x:Uid="Setter_258" Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter x:Uid="Setter_259" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_28" TargetType="{x:Type CheckBox}">
                    <Grid x:Uid="Grid_12">
                        <Grid.RowDefinitions>
                            <RowDefinition x:Uid="RowDefinition_5" Height="auto"/>
                            <RowDefinition x:Uid="RowDefinition_6" Height="auto"/>
                        </Grid.RowDefinitions>

                        <Border x:Uid="CheckBoxHotReign" x:Name="CheckBoxHotReign"
                                SnapsToDevicePixels="True"
                                Background="Transparent"
                                Margin="-5"
                                Width="28"
                                Height="28">
                            <BulletDecorator x:Uid="BulletDecorator_3" Background="Transparent"
                                             SnapsToDevicePixels="true"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Center">
                                <BulletDecorator.Bullet>
                                    <Image x:Uid="Image_6" x:Name="cb"
                                           Height="16" Width="16"
                                           Source="{controls:MultiDPIImageSource false}"
                                           SnapsToDevicePixels="true"
                                           Visibility="Hidden"/>
                                </BulletDecorator.Bullet>
                            </BulletDecorator>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <DataTrigger x:Uid="DataTrigger_7" Binding="{Binding IsMouseOver, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Border}}}" Value="True">
                            <Setter x:Uid="Setter_263" TargetName="cb" Property="Visibility" Value="Visible"/>
                        </DataTrigger>
                        <Trigger x:Uid="Trigger_55" Property="IsChecked" Value="True">
                            <Setter x:Uid="Setter_265" TargetName="cb" Property="Visibility" Value="Visible"/>
                            <Setter x:Uid="Setter_266" TargetName="cb" Property="Source" Value="{controls:MultiDPIImageSource true}"/>
                            <Setter x:Uid="Setter_267" Property="Background" Value="{DynamicResource Brush.Background.Gray}"/>
                            <Setter x:Uid="Setter_268" Property="BorderBrush" Value="{DynamicResource Brush.Background.Gray}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_28" x:Key="SeparatorStyle"  TargetType="{x:Type Separator}">
        <Setter x:Uid="Setter_114" Property="Focusable"  Value="false" />
        <Setter x:Uid="Setter_115" Property="OverridesDefaultStyle"  Value="true" />
        <Setter x:Uid="Setter_116" Property="Margin"  Value="2,1,2,1" />
        <Setter x:Uid="Setter_117" Property="Template" >
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_19" TargetType="{x:Type Separator}">
                    <Border x:Uid="Border_22" Margin="0,0,0,0" BorderBrush="{DynamicResource Brush.Item.Border}" BorderThickness="0" SnapsToDevicePixels="True">
                        <Rectangle x:Uid="Rectangle_3" 
                                   Stroke="{DynamicResource Brush.Item.Border}" 
                                   Height="1" 
                                   StrokeThickness="1" 
                                   StrokeDashArray="4 4"
                                   SnapsToDevicePixels="True"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_29" x:Key="StateButtonBaseStyle" TargetType="{x:Type controls:StateButton}">
        <Style.Resources>
            <Style x:Uid="Style_30" x:Key="IconContentGridStyle" TargetType="{x:Type Grid}">
                <Setter x:Uid="Setter_118" Property="Visibility" Value="Visible"/>
                <Setter x:Uid="Setter_119" Property="Margin" Value="0"/>
                <Setter x:Uid="Setter_120" Property="MaxWidth" Value="15"/>
                <Setter x:Uid="Setter_121" Property="MaxHeight" Value="15"/>
            </Style>
            <Style x:Uid="Style_31" x:Key="FocusedBorderStyle" TargetType="{x:Type Border}">
                <Setter x:Uid="Setter_122" Property="Visibility" Value="Collapsed"/>
            </Style>
            <Style x:Uid="Style_32" x:Key="BlinkingIconStyle" TargetType="{x:Type Path}">
                <Setter x:Uid="Setter_123" Property="Visibility" Value="Collapsed"/>
            </Style>
        </Style.Resources>
        <Setter x:Uid="Setter_124" Property="Foreground" Value="{DynamicResource BaseForeground}" />
        <Setter x:Uid="Setter_125" Property="GenericColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_126" Property="MouseOverColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_127" Property="MouseDownColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_128" Property="ChangeColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_129" Property="SelectedHoverColor" Value="{DynamicResource Brush.SharedStyle.FlatButtonBase.ButtonBorder}"/>
        <Setter x:Uid="Setter_130" Property="Background" Value="Transparent" />
        <Setter x:Uid="Setter_131" Property="BorderBrush" Value="Transparent" />
        <Setter x:Uid="Setter_132" Property="BorderThickness" Value="0" />
        <Setter x:Uid="Setter_133" Property="CornerRadius" Value="0"/>
        <Setter x:Uid="Setter_134" Property="Width" Value="15"/>
        <Setter x:Uid="Setter_135" Property="Height" Value="15"/>
        <Setter x:Uid="Setter_214" Property="AutoState" Value="True"/>
        <Setter x:Uid="Setter_136" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_20" TargetType="{x:Type controls:StateButton}">
                    <Grid x:Uid="grid" 
                          SnapsToDevicePixels="True"
                          x:Name="grid">
                        <Border x:Uid="Border_17" Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding CornerRadius}"/>
                        <Grid x:Uid="ContentGrid" x:Name="ContentGrid" 
                              Margin="{TemplateBinding Padding}"
                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition x:Uid="ContentColumn" Width="Auto" x:Name="ContentColumn" />
                                <ColumnDefinition x:Uid="IconColumn" x:Name="IconColumn"/>
                            </Grid.ColumnDefinitions>

                            <ContentPresenter x:Uid="contentPresenter" x:Name="contentPresenter"                                          
                                              HorizontalAlignment="Center"                                          
                                              VerticalAlignment="Center"   
                                              TextBlock.Foreground="{TemplateBinding Foreground}">
                                <ContentPresenter.Resources>
                                    <Style x:Uid="Style_33" TargetType="{x:Type TextBlock}">
                                        <Setter x:Uid="Setter_137" Property="TextTrimming"
                                            Value="CharacterEllipsis" />
                                        <Setter x:Uid="Setter_138" Property="VerticalAlignment"
                                                Value="Center"/>
                                    </Style>
                                </ContentPresenter.Resources>
                            </ContentPresenter>

                            <Grid x:Uid="content" Grid.Column="1"
                                  x:Name="content">
                                <Path x:Uid="DefaultIcon" x:Name="DefaultIcon"
                                      Fill="{TemplateBinding GenericColor}"
                                      Data="{TemplateBinding StateDefaultIcon}"
                                      Stretch="Uniform"
                                      />
                                <Path x:Uid="ChangeIcon" x:Name="ChangeIcon"
                                      Fill="{TemplateBinding ChangeColor}"
                                      Data="{TemplateBinding StateChangeIcon}"
                                      Stretch="Uniform"
                                      Visibility="Collapsed"
                                      />
                                <Path x:Uid="BlinkingIcon" x:Name="BlinkingIcon"
                                      Fill="{TemplateBinding MouseDownColor}"
                                      Data="{TemplateBinding StateDefaultIcon}"
                                      Stretch="Uniform"
                                      Style="{StaticResource BlinkingIconStyle}" 
                                      />
                            </Grid>
                        </Grid>
                    </Grid>

                    <ControlTemplate.Triggers>
                        <MultiTrigger x:Uid="MultiTrigger_1">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_3" Property="IsMouseOver" Value="True" />
                                <Condition x:Uid="Condition_4" Property="State" Value="0" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_139" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseOverColor}"/>
                            <Setter x:Uid="Setter_140" Property="Fill" TargetName="DefaultIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseOverColor}" />
                        </MultiTrigger>

                        <Trigger x:Uid="Trigger_16" Property="IsPressed" Value="true">
                            <Setter x:Uid="Setter_141" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                            <Setter x:Uid="Setter_142" Property="Fill" TargetName="DefaultIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                            <Setter x:Uid="Setter_143" Property="Fill" TargetName="ChangeIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                        </Trigger>

                        <Trigger x:Uid="Trigger_17" Property="State" Value="1">
                            <Setter x:Uid="Setter_144" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MouseDownColor}" />
                            <Setter x:Uid="Setter_145" Property="Visibility" TargetName="DefaultIcon" Value="Collapsed" />
                            <Setter x:Uid="Setter_146" Property="Visibility" TargetName="ChangeIcon" Value="Visible" />
                        </Trigger>

                        <Trigger x:Uid="Trigger_54" Property="Tag" Value="eyeball">
                            <Setter x:Uid="Setter_256" Property="GenericColor" Value="{DynamicResource Brush.Item.Text.Gray}"/>
                            <Setter x:Uid="Setter_257" Property="MouseOverColor" Value="{DynamicResource Brush.Item.Text.Gray}"/>
                            <Setter x:Uid="Setter_269" Property="MouseDownColor" Value="{DynamicResource Brush.Item.Text.Gray}"/>
                            <Setter x:Uid="Setter_205" Property="ChangeColor" Value="{DynamicResource Brush.Item.Text.Gray}"/>
                            <Setter x:Uid="Setter_206" Property="SelectedHoverColor" Value="{DynamicResource Brush.Item.Text.Gray}"/>
                        </Trigger>

                        <MultiTrigger x:Uid="MultiTrigger_2">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_5" Property="State" Value="1" />
                                <Condition x:Uid="Condition_6" Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_147" TargetName="contentPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHoverColor}" />
                            <Setter x:Uid="Setter_148" Property="Fill" TargetName="ChangeIcon" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHoverColor}" />
                        </MultiTrigger>

                        <Trigger x:Uid="Trigger_18" Property="IsEnabled" Value="False">
                            <Setter x:Uid="Setter_149" Property="Opacity" TargetName="ContentGrid" Value="0.3" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_34" x:Key="ButtonToolTip" TargetType="{x:Type ToolTip}" >
        <Setter x:Uid="Setter_150" Property="OverridesDefaultStyle" Value="true" />
        <Setter x:Uid="Setter_159" Property="SnapsToDevicePixels" Value="True" />
        <Setter x:Uid="Setter_151" Property="HasDropShadow" Value="True" />
        <Setter x:Uid="Setter_154" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_21" TargetType="{x:Type ToolTip}" >
                    <StackPanel x:Uid="StackPanel_2">
                    <Border x:Uid="Border_18" CornerRadius="0"
                            SnapsToDevicePixels="True"
                            BorderThickness="1"
                            Margin="2"
                            Width="{TemplateBinding Width}"
                            Height="{TemplateBinding Height}"
                            BorderBrush="{DynamicResource Brush.Tooltip.Border}"
                            Background="{DynamicResource Brush.Tooltip.Background}">
                        <ContentPresenter x:Uid="ContentPresenter_6" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Top" />
                    </Border>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_35" x:Key="ToolTipTextBlock" TargetType="{x:Type TextBlock}">
        <Setter x:Uid="Setter_230" Property="MaxWidth" Value="220"/>
        <Setter x:Uid="Setter_229" Property="TextWrapping" Value="Wrap"/>
        <Setter x:Uid="Setter_155" Property="Foreground" Value="{DynamicResource Brush.Tooltip.Text}" />
        <Setter x:Uid="Setter_156" Property="FontFamily" Value="{StaticResource Shared.FontFamily.SegoeUI}" />
        <Setter x:Uid="Setter_157" Property="FontSize" Value="{StaticResource Shared.FontSize.12px}" />
    </Style>


    <Style x:Uid="Style_38" x:Key="NormalTextBlockStyle" TargetType="TextBlock">
        <Setter x:Uid="Setter_207" Property="Foreground" Value="{DynamicResource Brush.Item.Text.Gray}"/>
        <Setter x:Uid="Setter_208" Property="FontFamily" Value="{StaticResource Shared.FontFamily.SegoeUI}"/>
        <Setter x:Uid="Setter_209" Property="FontSize" Value="{StaticResource Shared.FontSize.12px}"/>
        <Style.Triggers>
            <Trigger x:Uid="Trigger_43" Property="IsEnabled" Value="True">
                <Setter x:Uid="Setter_210" Property="Foreground" Value="{DynamicResource Brush.Item.Text.Black}"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Uid="Style_39" x:Key="TitleTextBlockStyle" TargetType="TextBlock">
        <Setter x:Uid="Setter_211" Property="Foreground" Value="{DynamicResource Brush.Title}"/>
        <Setter x:Uid="Setter_212" Property="FontFamily" Value="{StaticResource Shared.FontFamily.SegoeUI}"/>
        <Setter x:Uid="Setter_213" Property="FontSize" Value="{StaticResource Shared.FontSize.14px}"/>
    </Style>

    <Style x:Uid="Style_40" x:Key="PasswordBox_Style" TargetType="{x:Type PasswordBox}">
        <Setter x:Uid="Setter_215" Property="HorizontalContentAlignment" Value="Left" />
        <Setter x:Uid="Setter_216" Property="Padding" Value="1,3"/>
        <Setter x:Uid="Setter_217" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_24" TargetType="{x:Type PasswordBox}">
                    <Border x:Uid="Border_23" BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="2"
                            CornerRadius="0"
                            Background="{TemplateBinding Background}"
                            SnapsToDevicePixels="True">
                        <Grid x:Uid="Grid_9">
                            <Border x:Uid="Border_24" BorderBrush="{DynamicResource Brush.Item.Border}"
                                    BorderThickness="1,1,0,0"/>
                            <ScrollViewer x:Uid="ScrollViewer_2" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                          x:Name="PART_ContentHost"
                                          MaxWidth="230"
                                          Padding="{TemplateBinding Padding}"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="Center" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_46" Property="IsEnabled" Value="false">
                            <Setter x:Uid="Setter_220" Property="Opacity" Value="0.5" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_45" x:Key="RadioButtonStyle"
                   BasedOn="{StaticResource FlatRadioButtonStyle}"
                   TargetType="RadioButton">
        <Setter x:Uid="Setter_243" Property="Width" Value="Auto"/>
        <Setter x:Uid="Setter_244" Property="Margin" Value="15,2,0,2"/>
        <Setter x:Uid="Setter_245" Property="HorizontalAlignment" Value="Left"/>
        <Setter x:Uid="Setter_246" Property="FontFamily" Value="{StaticResource Shared.FontFamily.SegoeUI}"/>
        <Setter x:Uid="Setter_247" Property="FontSize" Value="{StaticResource Shared.FontSize.12px}"/>
        <Setter x:Uid="Setter_248" Property="IsEnabled" Value="{Binding ElementName=changePDFCheckbox, Path=IsChecked}"/>
        <Style.Triggers>
            <Trigger x:Uid="Trigger_49" Property="IsEnabled" Value="False">
                <Setter x:Uid="Setter_249" Property="Foreground" Value="{DynamicResource Brush.Item.Text.Gray}"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- SubmenuItem -->
    <ControlTemplate x:Uid="ControlTemplate_27" x:Key="CustomSubmenuItemTemplateKey" TargetType="{x:Type MenuItem}">
        <Border x:Uid="Border_29" Name="Border" BorderThickness="1" BorderBrush="Transparent">
            <Grid x:Uid="Grid_11">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition x:Uid="ColumnDefinition_3" Width="Auto" SharedSizeGroup="Icon"/>
                    <ColumnDefinition x:Uid="ColumnDefinition_4" Width="*" />
                    <ColumnDefinition x:Uid="ColumnDefinition_5" Width="Auto" SharedSizeGroup="Shortcut"/>
                    <ColumnDefinition x:Uid="ColumnDefinition_6" Width="13"/>
                </Grid.ColumnDefinitions>
                <ContentPresenter x:Uid="Icon" Name="Icon"
                                  Margin="6,0,6,0"
                                  VerticalAlignment="Center"
                                  ContentSource="Icon"/>
                <ContentPresenter x:Uid="HeaderHost" Name="HeaderHost"
                                  Grid.Column="1"
                                  ContentSource="Header"
                                  RecognizesAccessKey="True"/>
                <TextBlock x:Uid="InputGestureText" x:Name="InputGestureText"
                           Grid.Column="2"
                           Text="{TemplateBinding InputGestureText}"
                           Margin="5,2,0,2"
                           DockPanel.Dock="Right" />
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger x:Uid="Trigger_50" Property="Icon" Value="{x:Null}">
                <Setter x:Uid="Setter_250" TargetName="Icon" Property="Visibility" Value="Hidden"/>
            </Trigger>
            <Trigger x:Uid="Trigger_51" Property="IsHighlighted" Value="true">
                <Setter x:Uid="Setter_251" TargetName="Border" Property="Background" Value="{DynamicResource Brush.FileMenuItemButton.Background}"/>
                <Setter x:Uid="Setter_252" TargetName="Border" Property="BorderBrush" Value="{DynamicResource Brush.FileMenuItemButton.BorderBrush}"/>
            </Trigger>
            <Trigger x:Uid="Trigger_52" Property="IsEnabled" Value="false">
                <Setter x:Uid="Setter_253" Property="Foreground" Value="{DynamicResource Brush.Custom.MenuItem.Foreground}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- MenuItem Style -->
    <Style x:Uid="Style_46" x:Key="CustomMenuItemStyle" TargetType="{x:Type MenuItem}">
        <Setter x:Uid="Setter_254" Property="OverridesDefaultStyle" Value="True"/>
        <Style.Triggers>
            <Trigger x:Uid="Trigger_53" Property="Role" Value="SubmenuItem">
                <Setter x:Uid="Setter_255" Property="Template" Value="{StaticResource CustomSubmenuItemTemplateKey}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    
</ResourceDictionary>